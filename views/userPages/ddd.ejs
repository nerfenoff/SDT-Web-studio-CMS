<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>Box personal portfolio Template</title>
    <link rel="icon" href="../public/Templates/BoxPortfolio/img/fav.png" type="image/x-icon">

    <!-- Bootstrap -->
    <link href="../public/Templates/BoxPortfolio/css/bootstrap.min.css" rel="stylesheet">
    <link href="../public/Templates/BoxPortfolio/ionicons/css/ionicons.min.css" rel="stylesheet">
    <link href="../public/Templates/BoxPortfolio/css/editstyle.css" rel="stylesheet">
    <!-- main css -->
    <link href="../public/Templates/BoxPortfolio/css/style.css" rel="stylesheet">


    <!-- modernizr -->
    <script src="js/modernizr.js"></script>

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body>

    <!-- Preloader -->
    <div id="preloader">
        <div class="pre-container">
            <div class="spinner">
                <div class="double-bounce1"></div>
                <div class="double-bounce2"></div>
            </div>
        </div>
    </div>
    <!-- end Preloader -->

    <div class="container-fluid">
        <!-- box header -->
        <header class="box-header">
            <div class="box-logo">

                <a href="index"><img src="../public/Templates/BoxPortfolio/img/logo.png" width="80" alt="Logo"></a>
              <!-- <button class="btn-style btn-1">Change logo</button> -->
            </div>
            <!-- box-nav -->
            <a class="box-primary-nav-trigger" href="#0">
                <span class="box-menu-text">Menu</span><span class="box-menu-icon"></span>
            </a>
            <!-- box-primary-nav-trigger -->
        </header>
        <!-- end box header -->

        <!-- nav -->
        <nav>
            <ul class="box-primary-nav">
                <li class="box-label">About me</li>

                <li><a href="index">Intro</a> <i class="ion-ios-circle-filled color"></i></li>
                <li><a href="#">About me</a></li>
                <li><a href="#">services</a></li>
                <li><a href="#">portfolio</a></li>
                <li><a href="#">contact me</a></li>



               <!-- <li class="box-label">Follow me</li>

                <li class="box-social"><a href="#0"><i class="ion-social-facebook"></i></a></li>
                <li class="box-social"><a href="#0"><i class="ion-social-instagram-outline"></i></a></li>
                <li class="box-social"><a href="#0"><i class="ion-social-twitter"></i></a></li>
                <li class="box-social"><a href="#0"><i class="ion-social-dribbble"></i></a></li>
            -->
            </ul>
        </nav>
        <!-- end nav -->

        <!-- box-intro -->

                    

        <section class="box-intro">
            <div class="table-cell">
            <!--
                
                        <input id = "in1" type="text" size="20">
                        <input id = "in1" type="text" size="20">
                        <input id = "in1" type="text" size="20">
                        <input id = "in1" type="text" size="20">
                        <button class="btn-style">Rename intro</button>
            -->
                <h1 class="box-headline letters rotate-2">
                    <span class="box-words-wrapper">
                        <b class="is-visible">Виталика.</b>
                        <b>&nbsp;дада.</b>
                        <b>кодер.</b>
                    </span>
                </h1>
                <h5>everything you need to build your personal portfolio</h5>
            </div>

            <div class="mouse">
                <div class="scroll"></div>
            </div>
        </section>
        <!-- end box-intro -->
    </div>

<div id="content">
    <!-- portfolio div -->

    <div class="portfolio-div">
        <div class="portfolio"> 
            <div class="no-padding portfolio_container">
                <!-- single work -->

                <%- _data %>
                <!-- ########### HERE ########### -->


                <div id="AddCont" class="col-md-3 col-sm-6 graphics ads">
                    <a id="AddContA" href="ContentSelector?url=" + URL class="portfolio_item">
                        <img src="../public/Templates/BoxPortfolio/img/plus.png" alt="image" class="img-responsive" />
                        <div class="portfolio_item_hover addimg">
                            <div class="portfolio-border clearfix">
                                <div class="item_info">
                                    <span>add image</span>
                                    <em>Graphics / Ads</em>
                                </div>
                            </div>
                        </div>
                    </a>
                </div>
                



                <!-- end single work -->
            </div>
            <!-- end portfolio_container -->
       </div>
      <!--   portfolio -->
    </div>
    <!-- end portfolio div -->

</div>
<script id="JSDelete" type="text/javascript">
    document.getElementById('AddContA').href="ContentSelector?url=" +  document.location.pathname;
    document.getElementById('JSDelete').remove();
</script>
    <!-- footer -->
    <footer>
        <div class="container-fluid">
            <p class="copyright">© Box Portfolio 2016</p>
        </div>
    </footer>
    <!-- end footer -->

    <!-- back to top -->
    <a href="#0" class="cd-top"><i class="ion-android-arrow-up"></i></a>
    <!-- end back to top -->



    <!-- jQuery -->
    <script src="../public/Templates/BoxPortfolio/js/jquery-2.1.1.js"></script>
    <!--  plugins -->
    <script src="../public/Templates/BoxPortfolio/js/bootstrap.min.js"></script>
    <script src="../public/Templates/BoxPortfolio/js/menu.js"></script>
    <script src="../public/Templates/BoxPortfolio/js/animated-headline.js"></script>
    <script src="../public/Templates/BoxPortfolio/js/isotope.pkgd.min.js"></script>


    <!--  custom script -->
    <script src="../public/Templates/BoxPortfolio/js/custom.js"></script>

</body>

</html>